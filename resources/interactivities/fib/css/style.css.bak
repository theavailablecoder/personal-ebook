* {
  outline: none;
  padding: 0px;
  margin: 0px;
}
body {
  margin: 0px;
  padding: 0px;
  color: #000;
  font-size: 15px;
  font-style: normal;
  font-weight: 400;
  letter-spacing: normal;
  font-family: "Conv_trebuc";
}
html,
body {
  height: 100%;
}
.clear {
  clear: both;
}

#wrapper {
  width: 100%;
  height: 100%;
  background: #fee9ea url("../img/bg-pattern.png") no-repeat center center;
  position: relative;
  margin: 0px auto;
  max-width: 1024px;
  /* border-top: 4px #000 solid;
  border-bottom: 4px #000 solid; */
  background-size: 100% 100%;
}

#questions-list {
  width: 100%;
  height: 100%;
  /* background: rgb(255, 255, 255, 0.8); */
  position: relative;
  margin: 0px auto;
  margin-left:16px;
  padding: 1rem;
  max-width: 1024px;
  border-top: 4px rgb(255, 255, 255, 0.8);
  border-bottom: 4px rgb(255, 255, 255, 0.8);
  border-radius: 10px;
}

header {
  position: relative;
  background: url("../img/heading-bg.png") no-repeat;
  background-size: 100% 100%;
  width: 100%;
  height: 85px;
}
.heading {
  position: relative;
  padding: 0px 10px;
}
.heading h2 {
  margin: auto;
  color: #fff;
  display: inline-block;
  /* font-family: Walkman ankush; */
  font-weight: 400;
  /* font-size: 26px; */
  font-size: 2.5rem;
  padding: 25px 10px;
}

.heading h3 {
  margin: auto;
  color: #fff;
  display: inline-block;
  /* font-family: Walkman ankush; */
  font-weight: 300;
  /* font-size: 26px; */
  padding: 7px 10px;
}

.heading-num h2 {
  margin: auto;
  color: green;
  display: inline-block;
  font-family: "Conv_SwitzerlandCondBlack";
  font-weight: 300;
  font-size: 25px;
  padding: 8px 38px;
}

.content {
  padding: 20px 20px 10px;
  position: relative;
}

footer {
  position: relative;
  background: url(../img/footer-bg.png) no-repeat;
  background-size: 100% 100%;
  height: 100px;
  clear: both;
}
footer .buttons {
  float: right;
  padding-top: 35px;
  padding-right: 40px;
}

.btn-default {
  width: 108px;
  height: 36px;
  border-radius: 0px;
  background: url("../img/button-bg.png") no-repeat;
  border: 0;
  color: #000;
  text-align: center;
  font-size: 22px;
  font-weight: 300;
  margin: 10px 10px;
  padding: 10px 10px;
  transition: all 0.2s ease;
  font-family: "Titan One";
  line-height: 10px;
  outline: none;
}

.btn-default:focus,
.btn-default:active {
  outline: none;
  color: #000;
}

.btn-default:hover {
  border-radius: 0px;
  background: url("../img/button-bg-hover.png") no-repeat;
  color: #000;
  outline: none;
  border: 0;
  outline: none;
}

.ui-droppable-active {
  background: rgba(0, 0, 0, 0.1) !important;
}

img {
  max-width: 100%;
}
.modal-header {
  border: 0;
  padding: 0;
  display: none;
}

.modal-content {
  width: 500px;
  height: 500px;
  background: url("../../popup/welldone.png") no-repeat;
  text-align: center;
  box-shadow: none;
  border: 0;
  margin: auto;
  background-size: 100%;
  max-width: 100%;
}

.gamewin-msg {
  padding: 100px 20px 20px;
}

.gamewin-msg h1 {
  margin: auto;
  font-family: "Conv_Anderson Torchy The Battery Boy";
  color: #00ccff;
  font-size: 58px;
  text-shadow: 1px 5px 5px #000000;
  letter-spacing: 1px;
}

.score {
  position: absolute;
  top: 80px;
  right: 5px;
  display: inline-block;
  width: 90px;
  height: 90px;
  border: 2px #fff solid;
  border-radius: 100%;
  text-align: center;
  color: #fff;
  box-shadow: 1px 1px 5px 2px #000000b5;
  background: rgb(9, 138, 4);
  display: none;
}

.score h4 {
  margin: 10px auto;
  line-height: 30px;
  position: relative;
  font-size: 22px;
  text-align: center;
}

.score h4:first-child:after {
  content: "";
  width: 50px;
  height: 1px;
  background: #fff;
  position: absolute;
  bottom: -4px;
  left: 50%;
  margin-left: -25px;
}

#balloon-wrap {
  position: fixed;
  bottom: 0;
  left: 0;
  right: 0;
  top: 0;
  z-index: -1;
}
.balloon {
  position: absolute;
}

/* KEYFRAMES */

@-webkit-keyframes animateBubble {
  0% {
    margin-top: 1000px;
  }
  100% {
    margin-top: -100%;
  }
}

@-moz-keyframes animateBubble {
  0% {
    margin-top: 1000px;
  }
  100% {
    margin-top: -100%;
  }
}

@-ms-keyframes animateBubble {
  0% {
    margin-top: 1000px;
  }
  100% {
    margin-top: -100%;
  }
}

@-o-keyframes animateBubble {
  0% {
    margin-top: 1000px;
  }
  100% {
    margin-top: -100%;
  }
}

@keyframes animateBubble {
  0% {
    margin-top: 1000px;
  }
  100% {
    margin-top: -100%;
  }
}

@-webkit-keyframes sideWays {
  0% {
    margin-left: 0px;
  }
  100% {
    margin-left: 30px;
  }
}

@-moz-keyframes sideWays {
  0% {
    margin-left: 0px;
  }
  100% {
    margin-left: 30px;
  }
}

@-ms-keyframes sideWays {
  0% {
    margin-left: 0px;
  }
  100% {
    margin-left: 30px;
  }
}

@-o-keyframes sideWays {
  0% {
    margin-left: 0px;
  }
  100% {
    margin-left: 30px;
  }
}

@keyframes sideWays {
  0% {
    margin-left: 0px;
  }
  100% {
    margin-left: 30px;
  }
}

/* ANIMATIONS */

.x1 {
  left: 2%;
  top: 80%;
  z-index: 9;
  -webkit-animation: animateBubble 12s linear infinite,
    sideWays 2s ease-in-out infinite alternate;
  -moz-animation: animateBubble 12s linear infinite,
    sideWays 2s ease-in-out infinite alternate;
  -ms-animation: animateBubble 12s linear infinite,
    sideWays 2s ease-in-out infinite alternate;
  -o-animation: animateBubble 12s linear infinite,
    sideWays 2s ease-in-out infinite alternate;
  animation: animateBubble 12s linear infinite,
    sideWays 2s ease-in-out infinite alternate;
}

.x2 {
  left: 80%;
  top: 80%;
  z-index: 9;
  -webkit-animation: animateBubble 12s linear infinite,
    sideWays 4s ease-in-out infinite alternate;
  -moz-animation: animateBubble 12s linear infinite,
    sideWays 4s ease-in-out infinite alternate;
  -ms-animation: animateBubble 12s linear infinite,
    sideWays 4s ease-in-out infinite alternate;
  -o-animation: animateBubble 12s linear infinite,
    sideWays 4s ease-in-out infinite alternate;
  animation: animateBubble 12s linear infinite,
    sideWays 4s cubic-bezier(0.52, 0.04, 0.58, 1) infinite alternate;
}

.x3 {
  left: 90%;
  top: 60%;
  z-index: 9;
  -webkit-animation: animateBubble 18s linear infinite,
    sideWays 2s ease-in-out infinite alternate;
  -moz-animation: animateBubble 18s linear infinite,
    sideWays 2s ease-in-out infinite alternate;
  -ms-animation: animateBubble 18s linear infinite,
    sideWays 2s ease-in-out infinite alternate;
  -o-animation: animateBubble 18s linear infinite,
    sideWays 2s ease-in-out infinite alternate;
  animation: animateBubble 18s linear infinite,
    sideWays 2s ease-in-out infinite alternate;
}

.x4 {
  left: -10%;
  top: 60%;
  -webkit-animation: animateBubble 22s linear infinite,
    sideWays 3s ease-in-out infinite alternate;
  -moz-animation: animateBubble 22s linear infinite,
    sideWays 3s ease-in-out infinite alternate;
  -ms-animation: animateBubble 22s linear infinite,
    sideWays 3s ease-in-out infinite alternate;
  -o-animation: animateBubble 22s linear infinite,
    sideWays 3s ease-in-out infinite alternate;
  animation: animateBubble 22s linear infinite,
    sideWays 3s ease-in-out infinite alternate;
}

.x5 {
  left: 85%;
  top: 40%;
  -webkit-animation: animateBubble 25s linear infinite,
    sideWays 4s ease-in-out infinite alternate;
  -moz-animation: animateBubble 25s linear infinite,
    sideWays 4s ease-in-out infinite alternate;
  -ms-animation: animateBubble 25s linear infinite,
    sideWays 4s ease-in-out infinite alternate;
  -o-animation: animateBubble 25s linear infinite,
    sideWays 4s ease-in-out infinite alternate;
  animation: animateBubble 25s linear infinite,
    sideWays 4s ease-in-out infinite alternate;
}

.x6 {
  left: 0;
  top: 40%;
  -webkit-animation: animateBubble 25s linear infinite,
    sideWays 2s ease-in-out infinite alternate;
  -moz-animation: animateBubble 25s linear infinite,
    sideWays 2s ease-in-out infinite alternate;
  -ms-animation: animateBubble 25s linear infinite,
    sideWays 2s ease-in-out infinite alternate;
  -o-animation: animateBubble 25s linear infinite,
    sideWays 2s ease-in-out infinite alternate;
  animation: animateBubble 25s linear infinite,
    sideWays 2s ease-in-out infinite alternate;
}

/****/
@media (min-width: 768px) {
  .modal-dialog {
    width: 600px;
    margin: 100px auto 30px;
  }
}

@media screen and (max-width: 991px) {
  header {
    background: #7ba305;
    height: auto;
    padding: 5px 10px;
    border-bottom: 2px #fff solid;
  }

  .heading h2 {
    font-size: 25px;
    padding: 0;
  }

  footer {
    background: #7ba305;
    height: auto;
    padding: 5px 10px;
    border-top: 3px #fff solid;
  }

  footer .buttons {
    float: none;
    text-align: center;
    padding: 0;
  }
}

@media screen and (max-width: 767px) {
  .container-fluid {
    padding: 0;
  }
  .gamewin-msg h1 {
    font-size: 45px;
  }
  .heading h2 {
    font-size: 16px;
  }

  .heading {
    padding: 1px 10px;
  }

  .content {
    padding: 10px 20px 10px;
  }
  .btn-default {
    width: 90px;
    height: 30px;
    font-size: 18px;
  }
}

#feedbackModal {
  overflow: hidden;
}

